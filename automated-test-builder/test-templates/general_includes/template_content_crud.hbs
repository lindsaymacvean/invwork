---
title: UNSETVARIABLE CRUD
type: UNSETVARIABLE
typeURL: UNSETVARIABLE
---

{{#content "Edit Node"}}
	<!--Add node-->
	<tr>
		<td>open</td>
		<td>/node/add/{{type}}</td>
		<td></td>
	</tr>
	{{> enter_title_body}}
	{{> set_content_options}}
	{{> publish_node}}
{{/content}}

{{#content "Verify Node"}}
	<!--Verify node-->
	{{> verify_view_title_body}}
	<tr>
		<td>verifyElementPresent</td>
		<td>link=${security}</td>
		<td></td>
	</tr>
	<tr>
		<td>verifyElementPresent</td>
		<td>link=${relatedExternalLinksTitle}</td>
		<td></td>
	</tr>
{{/content}}

{{#content "Update Node"}}
	<!--Update the Node-->
	<tr>
		<td>clickAndWait</td>
		<td>link=Edit</td>
		<td></td>
	</tr>
	{{> verify_edit_title_body_update}}
	{{> enter_title_body_update}}
	{{> update_content_options}}
	{{#if userCantDelete}}
		{{> verify_cant_delete}}
	{{/if}}
	{{> publish_node}}
{{/content}}

{{#content "Verify Updated Node"}}
	<!--Verify node has been updated-->
	<tr>
		<td>open</td>
		<td>/{{typeURL}}/${titleUpdate}</td>
		<td></td>
	</tr>
	{{> verify_view_title_body_update}}
	<tr>
		<td>verifyText</td>
		<td>link=${securityUpdate}</td>
		<td>${securityUpdate}</td>
	</tr>
	<tr>
		<td>verifyText</td>
		<td>link=${relatedExternalLinksTitleUpdate}</td>
		<td>${relatedExternalLinksTitleUpdate}</td>
	</tr>
{{/content}}

{{#content "Delete Node"}}
	{{> delete_node}}
{{/content}}